{
  "name": "oauth-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@groundearth0/auth-utils": "^1.0.2",
    "aws-sdk": "^2.818.0",
    "axios": "^0.20.0",
    "cookie": "^0.4.1",
    "dayjs": "^1.9.8",
    "http-proxy-middleware": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.9",
    "netlify": "^2.4.0",
    "node-fetch": "^2.6.1",
    "querystring": "^0.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-google-login": "^5.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "2.1.5",
    "simple-oauth2": "^4.2.0",
    "time-ago": "^0.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm-run-all --parallel build:**",
    "build:app": "react-scripts build",
    "build:lambda": "tsc -p ./lambda",
    "auth-utils:install": "mkdir -p local-packages && cd local-packages && npm pack ../../auth-utils && cd .. && npm install $(ls ./local-packages/*.tgz) && rm ./local-packages/*",
    "dev": "export $(cat .env | xargs) && concurrently \"docker-compose up\" \"npm run build:lambda && netlify dev\" \"tsc -p ./lambda -w\" \"lt -h ${LOCALTUNNEL_HOST} -p 8888 -s barstool\"",
    "deploy": "npm run build && netlify deploy -p",
    "local": "export $(cat .env | xargs) && concurrently \"netlify dev\" \"lt -h ${LOCALTUNNEL_HOST} -p 8888 -s barstool\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/aws-lambda": "^8.10.68",
    "@types/cookie": "^0.4.0",
    "@types/gapi": "0.0.39",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.16",
    "@types/react": "^16.14.1",
    "@types/react-router": "^5.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/simple-oauth2": "^4.1.0",
    "concurrently": "^5.3.0",
    "dotenv-webpack": "^2.0.0",
    "localtunnel": "^2.0.0",
    "netlify-cli": "^2.69.11",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.1.3"
  }
}
